<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>


<BorderPane prefHeight="399.0" prefWidth="370.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19" fx:controller="application.SampleController">
   <center>
      <BorderPane prefHeight="514.0" prefWidth="618.0" BorderPane.alignment="CENTER">
         <center>
            <AnchorPane prefHeight="400.0" prefWidth="370.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="loginButton" layoutX="140.0" layoutY="197.0" mnemonicParsing="false" onAction="#openLogin" prefHeight="51.0" prefWidth="93.0" text="Login" textAlignment="CENTER">
                     <font>
                        <Font size="21.0" />
                     </font>
                  </Button>
                  <Button fx:id="signupButton" layoutX="142.0" layoutY="258.0" mnemonicParsing="false" onAction="#openSignup" prefHeight="48.0" prefWidth="86.0" text="Sign up" textAlignment="CENTER">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
                  <Label layoutX="109.0" layoutY="115.0" text="Welcome to UpTown Beverly!" />
                  <Label layoutX="189.0" layoutY="133.0" text=" A place where dreams are met." />
                  <ImageView fitHeight="344.0" fitWidth="453.0" layoutX="13.0" layoutY="-134.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../../Downloads/UpTown%20Builders-1.png" />
                     </image>
                  </ImageView>
                  <Button fx:id="adminlogin" layoutX="141.0" layoutY="323.0" mnemonicParsing="false" onAction="#openAdmin" text="Admin Log in" />
                  <Button fx:id="exitbutton" layoutX="316.0" layoutY="369.0" mnemonicParsing="false" onAction="#exitMenu" text="EXIT" />
               </children>
            </AnchorPane>
         </center>
      </BorderPane>
   </center>
</BorderPane>
